<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Serilog.UI</id>
    <version>$version$</version>
    <title>Serilog.UI</title>
    <authors>Mohsen Esmailpour</authors>
    <owners>mo.esmp</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <projectUrl>https://github.com/mo-esmp/serilog-ui</projectUrl>
    <license type="expression">MIT</license>
    <description>Simple web UI for several Serilog sinks.</description>
    <releaseNotes></releaseNotes>
    <copyright></copyright>
    <tags>serilog serilog-ui log logui log-ui web-log</tags>
    <dependencies>
      <group targetFramework=".NETCoreApp3.1">
        <dependency id="Microsoft.Extensions.DependencyInjection.Abstractions" version="3.1.3" exclude="Build,Analyzers" />
        <dependency id="Newtonsoft.Json" version="12.0.3" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NET5.0">
        <dependency id="Microsoft.Extensions.DependencyInjection.Abstractions" version="5.0.0" exclude="Build,Analyzers" />
        <dependency id="Newtonsoft.Json" version="12.0.3" exclude="Build,Analyzers" />
      </group>
    </dependencies>
    <frameworkReferences>
      <group targetFramework=".NETCoreApp3.1">
        <frameworkReference name="Microsoft.AspNetCore.App" />
      </group>
      <group targetFramework=".NET5.0">
        <frameworkReference name="Microsoft.AspNetCore.App" />
      </group>
    </frameworkReferences>
  </metadata>
  <files>
    <file src="bin\release\netcoreapp3.1\*.dll" target="lib/netcoreapp3.1" />
    <file src="bin\release\net5.0\*.dll" target="lib/net5.0" />
  </files>
</package>